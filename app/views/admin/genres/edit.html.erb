  <main>
    <%= render 'layouts/error', obj: @genre %>
    <div class="container">
    <h1>ジャンル編集</h1>
    </div>
    
    <div>
      <%= form_with model: @genre, url: admin_genre_path do |f| %>
      <table>
        <tr>
          <th class="head">ジャンル名</th>
          <td class="field"><%= f.text_field :name %></td>
          <td><%= f.submit '変更を保存', class:"button"%></td>

      </tr>
      </table>
      <% end %>
    </div>
    
  </main>